          "_customHTML": "<section class=\"table3\" group=\"Table\" plugins=\"amp-main, font-awesome\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"4\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"3\">\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Top Line\" name=\"showTopLine\">\n        <input type=\"color\" title=\" Line Color\" name=\"topLineColor\" value=\"#a2aab1\" condition=\"showTopLine\">\n        <select title=\"Cards\" name=\"cardsAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\" selected>3</option>\n        </select>\n                \n        <input type=\"color\" title=\"Card 1 Color\" name=\"card1Color\" value=\"#f3f3f3\">\n        <input type=\"color\" title=\"Card 2 Color\" name=\"card2Color\" value=\"#f3f3f3\" condition=\"cardsAmount==3 || cardsAmount==2\">\n        <input type=\"color\" title=\"Card 3 Color\" name=\"card3Color\" value=\"#f4f5f6\" condition=\"cardsAmount==3\">\n\n        <select title=\"Favorite Item\" name=\"favoriteItem\">\n            <option value=\"0\" selected>none</option>\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n        </select>\n\n        <input type=\"color\" title=\"Bookmark BG Color\" name=\"bookmarkBgColor\" value=\"#af9f8c\" condition=\"favoriteItem==1||favoriteItem==2||favoriteItem==3\">\n        <input type=\"color\" title=\"Bookmark Icon Color\" name=\"bookmarkIconColor\" value=\"#ffffff\" condition=\"favoriteItem==1||favoriteItem==2||favoriteItem==3\">\n        <input type=\"checkbox\" title=\"Show Type\" name=\"showcardTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Price\" name=\"showPrice\" checked>\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Show List String\" name=\"showListString\">\n        <input type=\"range\" inline title=\"String Count\" name=\"listGrow\" min=\"1\" max=\"5\" step=\"1\" value=\"5\" condition=\"showListString\">\n        <input type=\"checkbox\" title=\"Show Icons\" name=\"showIcons\" condition=\"showListString\" checked>\n        <input type=\"color\" title=\"Card 1 Counter Color\" name=\"Card1CounterColor\" value=\"#00dcc0\" condition=\" showIcons && showListString\">\n        <input type=\"color\" title=\"Card 2 Counter Color\" name=\"Card2CounterColor\" value=\"#00dcc0\" condition=\"(cardsAmount==3 || cardsAmount==2) && showIcons && showListString\">\n        <input type=\"color\" title=\"Card 3 Counter Color\" name=\"Card3CounterColor\" value=\"#00dcc0\" condition=\"cardsAmount==3 && showIcons && showListString\">\n        <input type=\"range\" inline title=\"Icon Size\" name=\"iconSize\" min=\"0.5\" max=\"1.5\" step=\"0.1\" value=\"1\" condition=\"showIcons\">\n        <input type=\"checkbox\" title=\"Show Underline\" name=\"showUnderline\">\n        \n        <input type=\"color\" title=\"Underline Color\" name=\"underlineColor\" value=\"#767676\" condition=\"showUnderline\">\n\n        <input type=\"checkbox\" title=\"Show Buttons\" name=\"showButtons\" checked>\n\n        <input type=\"checkbox\" title=\"Show Text After Cards\" name=\"textAfterCards\" checked>\n\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/background4-1.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#c8d200\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input typ